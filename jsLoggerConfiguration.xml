<?xml version="1.0" encoding="ISO-8859-1" ?>
<jsLogger>
	<blocks>
		<defaultValues>
			<valid-duration>300</valid-duration><!--  Valid duration in seconds -->
			<run-once>false</run-once>
			<window-size>3</window-size>
		</defaultValues>
	</blocks>
	<associatedIdLength>15</associatedIdLength>
	<databaseFile>./jslogger-data.db</databaseFile>
	<persistData>true</persistData> <!--  If set to false, will not support static files either -->
	<rateLimit>
		<pastMinutesToKeep>5</pastMinutesToKeep>
		<dataLimit>5</dataLimit> <!--  in kb/minute -->
		<logsLimit>500</logsLimit> <!-- in logs/minute -->
	</rateLimit>
</jsLogger>